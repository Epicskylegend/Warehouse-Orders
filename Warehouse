public class Warehouse {
    public double widgetShipPrice = 5.50; // Price of buying widgets to add to warehouse
    public double widgetOrderPrice = 5.50 * 1.5; // Price of selling widgets which is 50% above the buying cost.

    public static void main(String[] args) {

        double widgetShipPrice = 5.5;
        double widgetOrderPrice = widgetShipPrice * 1.5;

    }
}

